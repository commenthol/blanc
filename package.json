{
  "name": "blanc",
  "version": "0.6.0",
  "private": true,
  "description": "node module boilerplate",
  "keywords": [
    "boilerplate",
    "module"
  ],
  "homepage": "https://github.com/commenthol/blanc",
  "bugs": {
    "url": "https://github.com/commenthol/blanc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/blanc.git"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "src/index.js",
  "types": "types",
  "bin": {},
  "man": [],
  "files": [
    "dist",
    "src",
    "types"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "ci": "npm-run-all clean lint build coverage types",
    "clean": "rimraf dist lib es coverage",
    "clean:all": "npm run clean && rimraf node_modules",
    "coverage": "c8 -r text -r html npm test",
    "lint": "eslint --ext .js .",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha",
    "types": "rimraf types; tsc --emitDeclarationOnly true --noEmitOnError"
  },
  "mocha": {
    "checkLeaks": true,
    "colors": true,
    "exit": false
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/node": "^16.11.38",
    "c8": "^7.11.3",
    "core-js": "^3.22.8",
    "eslint": "^8.17.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.5",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
