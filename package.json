{
  "name": "blanc",
  "version": "0.5.0",
  "private": true,
  "description": "node module boilerplate",
  "keywords": [
    "boilerplate",
    "module"
  ],
  "homepage": "https://github.com/commenthol/blanc",
  "bugs": {
    "url": "https://github.com/commenthol/blanc/issues"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "dist",
  "bin": {},
  "man": [],
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "doc": "doc",
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/blanc.git"
  },
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm test",
    "build": "npm run build:cjs && npm run build:dist",
    "build:cjs": "babel -d lib src",
    "build:dist": "rollup -c rollup.conf.js",
    "clean": "rimraf lib es coverage .nyc_output",
    "clean:all": "rimraf node_modules && npm run clean",
    "coverage": "nyc -r text -r html npm test",
    "doc": "jsdoc -c scripts/jsdoc/jsdoc.json",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0",
    "core-js": "^2.5.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=4.0.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
