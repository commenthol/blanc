{
  "name": "blanc",
  "version": "0.7.0",
  "private": true,
  "description": "node module boilerplate",
  "keywords": [
    "boilerplate",
    "module"
  ],
  "homepage": "https://github.com/commenthol/blanc",
  "bugs": {
    "url": "https://github.com/commenthol/blanc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/blanc.git"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "src/index.js",
  "types": "types",
  "bin": {},
  "man": [],
  "files": [
    "dist",
    "src",
    "types"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "ci": "npm-run-all clean lint build coverage types",
    "clean": "rimraf dist lib es coverage",
    "clean:all": "npm run clean && rimraf node_modules",
    "coverage": "c8 -r text -r html -r lcov npm test",
    "lint": "eslint --ext .js .",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha",
    "types": "rimraf types; tsc"
  },
  "mocha": {
    "checkLeaks": true,
    "colors": true,
    "exit": false
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/node": "^20.4.8",
    "c8": "^8.0.1",
    "core-js": "^3.32.0",
    "eslint": "^8.46.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
