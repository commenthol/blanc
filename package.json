{
  "name": "blanc",
  "version": "0.5.0",
  "private": true,
  "description": "node module boilerplate",
  "keywords": [
    "boilerplate",
    "module"
  ],
  "homepage": "https://github.com/commenthol/blanc",
  "bugs": {
    "url": "https://github.com/commenthol/blanc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/blanc.git"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "dist",
  "bin": {},
  "man": [],
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "doc": "doc",
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm test",
    "build": "npm run build:cjs && npm run build:dist",
    "build:cjs": "babel -d lib src",
    "build:dist": "rollup -c rollup.config.js",
    "clean": "rimraf lib es coverage .nyc_output",
    "clean:all": "npm run clean && rimraf node_modules",
    "coverage": "nyc -r text -r html npm test",
    "doc": "jsdoc -c scripts/jsdoc/jsdoc.json",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha"
  },
  "mocha": {
    "checkLeaks": true,
    "require": "@babel/register"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=4.0.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
