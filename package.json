{
  "name": "blanc",
  "version": "0.3.0",
  "description": "node module boilerplate",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "istanbul": "^0.4.1",
    "jsdox": "latest",
    "mocha": "^2.3.4"
  },
  "scripts": {
    "prepublish": "npm run lint && npm run test",
    "test": "mocha --reporter spec --bail --check-leaks test/*.mocha.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- --reporter dot --check-leaks test/*.mocha.js",
    "doc": "jsdox -o doc index.js lib/*.js",
    "lint": "eslint --ignore-path .gitignore */*.js **/*.js",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "clean": "rm -rf doc coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/blanc.git"
  },
  "keywords": [
    "module",
    "boilerplate"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commenthol/blanc/issues"
  },
  "homepage": "https://github.com/commenthol/blanc",
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  }
}
